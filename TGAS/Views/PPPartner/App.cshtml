@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" class="ng-app: pApp">
<head>
    <meta charset="UTF-8">
    <!--IE兼容标签-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--响应式布局-->
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=0, minimal-ui" />
    <!--启动webapp功能，会删除默认的苹果工具栏和菜单栏-->
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <!--启动webapp功能时，显示手机信号、时间、电池的顶部导航栏的颜色-->
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <!--忽略页面中的数字识别为电话号码-->
    <meta name="format-detection" content="telephone=no, email=no" />
    <!--默认使用webkit内核进行渲染-->
    <meta name="renderer" content="webkit">
    <title>PPmoney理财平台</title>
</head>
<body ng-controller="partnerCtrl">
    <div ng-view=""></div>
    <link rel="stylesheet" href="/app/styles/base.css" />
    <link rel="stylesheet" href="/app/styles/weixin-grid.css" />
    <link rel="stylesheet" href="/app/styles/weixin-panel.css" />
    <link rel="stylesheet" href="/app/styles/weixin-btn.css" />
    <link rel="stylesheet" href="/app/styles/weixin-icon.css" />
    <link rel="stylesheet" href="/app/styles/partner/partnermain.css" />
    <script data-main="/app/scripts/routes/partnerMain" src="/app/scripts/vendor/require.js"></script>
</body>
</html>

